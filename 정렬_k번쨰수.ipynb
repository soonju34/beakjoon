    "def solution(array, commands):\n",
    "    answer = []\n",
    "    \n",
    "    for i in commands:\n",
    "        ary = array[i[0]-1: i[1]]    # 문제에서 주어진 크기만큼 자르기\n",
    "        ary.sort()    # sort 함수로 정렬\n",
    "        answer.append(ary[i[2]-1])    # k 번째 값 집어넣기\n",
    "        \n",
    "    return answer"
   ]
  }
