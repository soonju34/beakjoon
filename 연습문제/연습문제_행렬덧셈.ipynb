    "    answer = []\n",
    "    \n",
    "    for i in range(len(arr1)):\n",
    "        arr_sum = []\n",
    "        for j in range(len(arr1[0])):\n",
    "            arr_sum.append(arr1[i][j] + arr2[i][j])\n",
    "        answer.append(arr_sum)\n",
    "            \n",
    "    return answer"
